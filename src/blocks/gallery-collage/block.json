{
	"name": "coblocks/gallery-collage",
	"category": "coblocks-galleries",
	"attributes": {
		"images": {
			"type": "array",
			"default": [],
			"source": "query",
			"selector": ".wp-block-coblocks-gallery-collage__item",
			"query": {
				"url": {
					"source": "attribute",
					"selector": "img",
					"attribute": "src"
				},
				"link": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-link"
				},
				"imgLink": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-imglink"
				},
				"alt": {
					"source": "attribute",
					"selector": "img",
					"attribute": "alt",
					"default": ""
				},
				"id": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-id"
				},
				"index": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-index"
				},
				"caption": {
					"type": "array",
					"source": "children",
					"selector": "figcaption"
				}
			}
		},
		"gutter": {
			"type": "string",
			"default": "small"
		},
		"shadow": {
			"type": "string",
			"default": "none"
		}
	}
}
